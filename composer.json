{
    "name": "poro_bp/newscrud",
    "type": "library",
    "description": "Poro admin panel for news with categories and tags, using Backpack\\CRUD on Laravel 5.",
    "keywords": [
        "backpack",
        "NewsCRUD"
    ],
    "require": {
        "backpack/crud": "^3.0.6",
        "cviebrock/eloquent-sluggable": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "PoroBp\\NewsCRUD\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PoroBp\\NewsCRUD\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "PoroBp\\NewsCRUD\\NewsCRUDServiceProvider"
            ]
        }
    }
}
